#include <iostream>
#include <ctime>
using namespace std;


int main ()
{


	int viti;                          
	int muaji;
	int javet;
	int ditet;
	int oret;
	int minutat;
	int sekondat;
	int data;
	
	 cout << "Fut vitin e lindjes,muajin, diten dhe oren :  \n";
 



 cout << "Vendos vitin e lindjes : ";
 cin >> data;


 
 
 
 
 data = data - 2018;
 
 
 data = data * (-1);
 
 viti = data;
 
 muaji = viti * 12;
 
 javet = muaji * 4;
 
 ditet = javet * 24;
 
 oret = ditet * 60;
 
 minutat = oret * 60;
 
 sekondat = minutat * 60;
 
 cout << "Ju jeni :" << viti << " vjec \n" << muaji << " muaj \n" << javet << " jave \n" << ditet << " dite \n" << oret << " ore \n" << minutat << " minuta \n" << sekondat << " sekonda";
	
	
	
	
	
	
	
	
	return 0;
}
